<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<context:annotation-config />
	<bean id="Aboj2" class="Beans.Address">
	<property name="Aid" value="1"></property>
	</bean>
	
	
	<bean  id="Aob" class="Beans.Address">
	<property name="Aid" value="20"></property>
	</bean>
	
	<bean id="Eobj" class="Beans.Employee">
	<property name="name" value="Ajinkya"></property>
	</bean>
	</beans>